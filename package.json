{
  "name": "redux-crud-resources",
  "version": "0.0.1",
  "description": "programmatically generate actions, action creators, and reducers for a given list of resources",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist lib",
    "prebuild": "npm run clean",
    "lib-build": "babel index.js --out-dir lib",
    "build": "npm run lib-build",
    "lint": "eslint --cache --fix --ext .js --ext .js src",
    "test": "nyc --all mocha --recursive -R nyan --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "test"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "keywords": [
    "redux",
    "react",
    "boilerplate"
  ],
  "author": "Zach Pflederer",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bluebird": "^3.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "expect": "^1.20.2",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  }
}
